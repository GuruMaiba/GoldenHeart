@model ICollection<GoldenHeart.Models.MessageStaff>

@{
    ViewBag.Title = "Информация для персонала";
    ViewBag.viewSubmenu = true;
}

<div class="issues-personal">
    <div class="issues-personal-fon">
        @if (Model.Count > 0)
        {
            foreach (var mess in Model)
            {
                <div class="issues-personal-item">
                    <h3>Администратор</h3>
                    <span>@mess.DateCreate.ToString("[ dd.MM.yyyy | HH:mm ]")</span>
                    <p>@mess.Text</p>
                </div>
            }
        }
        else
        {
            <div class="issues-personal-item">
                <h3>Новая информация отсутствует</h3>
            </div>
        }
    </div>
</div>

